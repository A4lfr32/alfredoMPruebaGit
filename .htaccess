
# DO NOT REMOVE THIS LINE AND THE LINES BELOW SSL_REDIRECT:alfredomaussa.com
RewriteEngine on
RewriteCond %{HTTPS} ^alfredomaussa.com [NC]
RewriteCond %{HTTP_HOST} ^alfredomaussa.com [NC]
RewriteRule ^(.*)$ http://www.alfredomaussa.com/$1 [L,R=301]
# DO NOT REMOVE THIS LINE AND THE LINES ABOVE SSL_REDIRECT:alfredomaussa.com
<IfModule mod_rewrite.c>
RewriteEngine on
